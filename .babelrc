{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@": "./",
          "public": "./public",
          "@images": "./public/images",
          "@svg": "./public/svg",
          "@styles": "./src/styles",
          "@comp": "./src/components"
        }
      }
    ]
  ]
}